{
    "name": "yiisoft/app",
    "type": "project",
    "description": "Yii3 Demo Blog",
    "keywords": [
        "yii3",
        "demo",
        "application"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/app/issues?state=open",
        "source": "https://github.com/yiisoft/app",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "chat": "https://t.me/yii3en"
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/yiisoft"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/yiisoft"
        }
    ],
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@php ./yii serve"
        ],
        "test": "codecept run",
        "psalm": "psalm",
        "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php --diff",
        "rector": "rector",
        "composer-dependency-analyser": "composer-dependency-analyser"
    },
    "require": {
        "php": "^8.4",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "httpsoft/http-message": "^1.1.6",
        "psr/container": "^2.0.2",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0.2",
        "psr/http-server-middleware": "^1.0.2",
        "psr/log": "^3.0.2",
        "ramsey/uuid": "^4.9.1",
        "symfony/console": "^7.3.3",
        "vlucas/phpdotenv": "^5.6.2",
        "webmozart/assert": "^1.11",
        "yiisoft/access": "^2.0",
        "yiisoft/aliases": "^3.1",
        "yiisoft/arrays": "^3.2",
        "yiisoft/assets": "^5.1.1",
        "yiisoft/auth": "^3.2",
        "yiisoft/cache-file": "^3.2",
        "yiisoft/config": "^1.6",
        "yiisoft/csrf": "^2.2.2",
        "yiisoft/data": "dev-master",
        "yiisoft/data-db": "dev-master",
        "yiisoft/data-response": "^2.1.1",
        "yiisoft/db": "dev-master",
        "yiisoft/db-migration": "dev-master",
        "yiisoft/db-sqlite": "dev-master",
        "yiisoft/definitions": "^3.4",
        "yiisoft/di": "^1.4",
        "yiisoft/error-handler": "^4.3",
        "yiisoft/form": "^1.4",
        "yiisoft/form-model": "^1.0.2",
        "yiisoft/html": "^3.11",
        "yiisoft/http": "^1.2",
        "yiisoft/hydrator": "^1.6.1",
        "yiisoft/hydrator-validator": "^2.0.1",
        "yiisoft/injector": "^1.2",
        "yiisoft/input-http": "^1.0.1",
        "yiisoft/log": "^2.1.1",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/middleware-dispatcher": "^5.3",
        "yiisoft/rbac": "^2.1",
        "yiisoft/rbac-db": "dev-db2",
        "yiisoft/rbac-php": "^2.0",
        "yiisoft/request-body-parser": "^1.1.1",
        "yiisoft/request-provider": "^1.2",
        "yiisoft/router": "^4.0.1",
        "yiisoft/router-fastroute": "^4.0.2",
        "yiisoft/security": "^1.1",
        "yiisoft/session": "^3.0",
        "yiisoft/strings": "^2.6",
        "yiisoft/user": "^2.3.1",
        "yiisoft/validator": "^2.5",
        "yiisoft/view": "^12.2.1",
        "yiisoft/widget": "^2.2",
        "yiisoft/yii-console": "^2.3",
        "yiisoft/yii-dataview": "dev-master",
        "yiisoft/yii-http": "^1.1.0",
        "yiisoft/yii-runner-console": "^2.2",
        "yiisoft/yii-runner-http": "^3.2",
        "yiisoft/yii-view-renderer": "^7.3"
    },
    "require-dev": {
        "codeception/c3": "^2.9",
        "codeception/codeception": "^5.3.2",
        "codeception/module-asserts": "^3.2",
        "codeception/module-cli": "^2.0.1",
        "codeception/module-phpbrowser": "^3.0.2",
        "friendsofphp/php-cs-fixer": "^3.87.2",
        "phpunit/phpunit": "^11.5.40",
        "rector/rector": "^2.1.7",
        "roave/infection-static-analysis-plugin": "^1.39",
        "roave/security-advisories": "dev-latest",
        "shipmonk/composer-dependency-analyser": "^1.8.3",
        "vimeo/psalm": "^6.13.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin-file": "config/configuration.php"
    },
    "config": {
        "sort-packages": true,
        "bump-after-update": true,
        "allow-plugins": {
            "yiisoft/config": true,
            "infection/extension-installer": true,
            "codeception/c3": true,
            "composer/installers": true
        }
    }
}
